all: cpu

cpu:
	make -j -C Fortran/ cpu

gpu:
	make -j -C Fortran/ gpu

fx:
	make -j -C Fortran/ fx

sx:
	make -j -C Fortran/ sx

run_cpu:
	make -j -C Fortran/ run_cpu

run_gpu:
	make -j -C Fortran/ run_gpu
	
run_fx:
	make -j -C Fortran/ run_fx

run_sx:
	make -j -C Fortran/ run_sx
	
clean:
	- make -j -C Fortran/ clean

